<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://levelsbeyond.com/schema/workflow"
          xmlns:nimbus="http://levelsbeyond.com/schema/workflow/nimbus"
          xsi:schemaLocation="http://levelsbeyond.com/schema/latest http://levelsbeyond.com/schema/latest/studio.xsd"
          id="Ex7rh"
          name="Ex7rh"
          executionLabelExpression="Ex7rh | "
          subjectDOClassName="AssetMaster"
          showInUserInterface="true"
          sdkVersion=""
>

    <initialStepName>asset collection</initialStepName>

    <noopStep name="asset collection"
              executionLabelExpression="the ${subject}"
              nextStep="any asset subflow"
    />

    <executeSubflowStep name="any asset subflow"
                        targetWorkflowId="_createProxiesAnyAsset"
                        subjectChangePath="${assetMaster}"
                        executionLabelExpression="display asset information for ${assetName}"
                        nextStep="display data"
                        >
<!--        <subflowContextDataMapping parentDataDef="mezzFile" subflowDataDef="mezzFile"/>-->
    </executeSubflowStep>

    <noopStep name="display data" executionLabelExpression="Asset mezzanine: ${subject.mezzanine}" nextStep="end" />


    <!-- ................................................... End Steps .................................................... -->
    <!-- success -->
    <noopStep name="end"/>


    <!-- ................................................... Data Defs .................................................... -->
    <!-- ............ INPUT ............. -->
    <contextDataDef name="assetMaster" dataType="Data Object" defaultDataExpression="${subject}" />
    <contextDataDef name="assetName"   dataType="String"      defaultDataExpression="${assetMaster.name" />
    <contextDataDef name="mezzFile"    dataType="File"        defaultDataExpression="${assetMaster.mezzanine}" />

    <!-- ......... PROCESSING ........... -->


</workflow>